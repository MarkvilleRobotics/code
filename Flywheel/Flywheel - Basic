#pragma config(Motor,  port1,           motor1,        tmotorVex393_HBridge, openLoop)
#pragma config(Motor,  port10,          motor2,        tmotorVex393_HBridge, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

//Hardware config

// Designed to start motor 1 at port 1 when ran


// This code only works autonomously under USB-Only Mode!
task main()
{
	for(;;){
		motor[motor1]=127;
		motor[motor2]=-127;
  	//motor[] sets a specified motor declared in hardware config to run at speed 127 for undefined amount of time.
		delay (1000);
		//Delay makes the microcontroller do nothing. It keeps doing what it was instructed (motor[]) for 1000 milliseconds (1 second).
	}
}//Program ends here. Motor stops moving when program is done.
